  "bugs": {
    "url": "https://github.com/subtirelumihail/react-fullpage/issues"
  },
  "homepage": "https://github.com/subtirelumihail/react-fullpage",
  "dependencies": {
    "babel-preset-stage-2": "^6.18.0",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-redux": "^5.0.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
    "babel-loader": "6.2.7",
